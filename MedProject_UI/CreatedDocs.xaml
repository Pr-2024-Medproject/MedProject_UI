<Window x:Class="MedProject_UI.CreatedDocs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedProject_UI"
        mc:Ignorable="d"
        Title="Існуючі документи" Height="428" Width="644">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="4*"/>
        </Grid.RowDefinitions>
        <Label x:Name="PatientName" Content="У пацієнта %% існують такі документи:" HorizontalAlignment="Left" Margin="25,0,0,0" VerticalAlignment="Center" FontSize="20"/>
        <ListBox x:Name="ListOfDocs" Grid.Row="1"/>

    </Grid>
</Window>
